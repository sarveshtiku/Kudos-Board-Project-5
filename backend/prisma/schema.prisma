datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

     model User {
       id       Int     @id @default(autoincrement())
       username String?
       email    String  @unique
       password String?
       googleId String?
       name     String?
       resetToken        String?
       resetTokenExpiry  DateTime?
     }